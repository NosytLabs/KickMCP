{
  "name": "kick-mcp",
  "version": "1.0.0",
  "description": "Kick MCP Server",
  "main": "dist/index.js",
  "bin": {
    "kick-mcp": "dist/bin/mcp.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "HTTP_MODE=true node dist/index.js",
    "dev": "HTTP_MODE=true ts-node-dev --respawn --transpile-only src/index.ts",
    "mcp": "ts-node src/bin/mcp.ts",
    "mcp:prod": "node dist/bin/mcp.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "express": "^4.18.2",
    "json-rpc-2.0": "^1.0.0",
    "ws": "^8.13.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "node-cache": "^5.1.2",
    "rate-limiter-flexible": "^2.4.1",
    "axios": "^1.6.0",
    "rimraf": "^5.0.5",
    "express-rate-limit": "^7.1.5",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/node-cache": "4.1.2",
    "@types/supertest": "^6.0.2",
    "typescript": "^5.3.2",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "eslint": "^8.55.0",
    "@typescript-eslint/parser": "^6.13.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NosytLabs/KickMCP.git"
  },
  "keywords": [
    "kick",
    "api",
    "mcp",
    "streaming"
  ],
  "author": "NosytLabs",
  "license": "MIT"
}
